<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkDialog" id="restraints_editor_dialog">
    <property name="width_request">400</property>
    <property name="height_request">460</property>
    <property name="visible">True</property>
    <property name="title" translatable="yes">Coot: Restraints Editor</property>
    <property name="modal">False</property>
    <property name="resizable">True</property>
    <child>
      <object class="GtkBox" id="dialog-vbox1">
        <property name="orientation">vertical</property>
        <property name="visible">True</property>
        <property name="homogeneous">False</property>
        <property name="spacing">0</property>
        <child>
          <object class="GtkBox" id="vbox1">
            <property name="orientation">vertical</property>
            <property name="visible">True</property>
            <property name="homogeneous">False</property>
            <property name="spacing">0</property>
            <child>
              <object class="GtkNotebook" id="restraints_editor_notebook">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="show_tabs">True</property>
                <property name="show_border">True</property>
                <property name="tab_pos">GTK_POS_TOP</property>
                <property name="scrollable">False</property>
                <property name="enable_popup">False</property>

                <!-- ooooooooooooooooooooooooooooooooo Information oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>
                    <child>
                      <object class="GtkTreeView" id="info_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Info</property>
                  </object>
                </child>

                <!-- ooooooooooooooooooooooooooooooooo Atoms oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="atoms_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Atoms</property>
                  </object>
                </child>

                <!-- ooooooooooooooooooooooooooooooooo Bonds oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="bonds_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Bonds</property>
                  </object>
                </child>

                <!-- ooooooooooooooooooooooooooooooooo Angles oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="angles_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Angles</property>
                  </object>
                </child>


                <!-- ooooooooooooooooooooooooooooooooo Torsion oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="torsions_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Torsions</property>
                  </object>
                </child>

                <!-- ooooooooooooooooooooooooooooooooo Chirals oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="chirals_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Chirals</property>
                  </object>
                </child>

                <!-- ooooooooooooooooooooooooooooooooo Planes oooooooooooooooooo -->

                <child>
                  <object class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="vexpand">1</property>
                    <property name="hexpand">1</property>

                    <child>
                      <object class="GtkTreeView" id="planes_treeview">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="headers_visible">True</property>
                        <property name="reorderable">False</property>
                        <property name="enable_search">True</property>
                        <property name="fixed_height_mode">False</property>
                        <property name="hover_selection">False</property>
                        <property name="hover_expand">False</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child type="tab">
                  <object class="GtkLabel">
                    <property name="label">Planes</property>
                  </object>
                </child>

              </object>
            </child>
          </object>
        </child>

        <child>
          <object class="GtkBox">
            <property name="orientation">horizontal</property>
            <property name="visible">True</property>
            <property name="margin-top">2</property>
            <property name="margin-bottom">2</property>
            <property name="margin-start">4</property>
            <property name="margin-end">4</property>

            <child>
              <object class="GtkButton" id="restraints_editor_add_restraint_button">
                <property name="visible">True</property>
                <property name="label">Add Restraint</property>
                <property name="visible">True</property>
                <property name="margin-top">4</property>
                <property name="margin-bottom">4</property>
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <signal name="clicked" handler="on_restraints_editor_add_restraint_button_clicked"/>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="restraint_editors_delete_restraint_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="label" translatable="yes">  Delete Restraint  </property>
                <property name="focus_on_click">True</property>
                <property name="visible">True</property>
                <property name="margin-top">4</property>
                <property name="margin-bottom">4</property>
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <signal name="clicked" handler="on_restraints_editor_delete_restraint_button_clicked"/>
              </object>
            </child>

          </object>
        </child>


        <child>
          <object class="GtkSeparator"/>
        </child>

        <child>
          <object class="GtkBox" id="dialog-action_area1">
            <property name="orientation">horizontal</property>
            <property name="visible">True</property>
            <property name="halign">end</property>
            <child>
              <object class="GtkButton" id="restraints_editor_close_button">
                <property name="visible">True</property>
                <property name="label" translatable="yes">Close</property>
                <property name="margin-top">4</property>
                <property name="margin-bottom">4</property>
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <signal name="clicked" handler="on_restraints_editor_close_button_clicked"/>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="restraints_editor_apply_button">
                <property name="label" translatable="yes">Apply</property>
                <property name="visible">False</property> <!-- we can't edit restraints yet -->
                <property name="margin-top">4</property>
                <property name="margin-bottom">4</property>
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <signal name="clicked" handler="on_restraints_editor_apply_button_clicked"/>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
